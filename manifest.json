{
    "name": "imagefd",
    "version": "1.0",
    "description": "image for darkmode",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content_scripts.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "magick.js"
    ],
    "manifest_version": 2
}